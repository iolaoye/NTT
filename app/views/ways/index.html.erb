<h1>Listing ways</h1>

<table>
  <tr>
    <th>Way name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ways.each do |way| %>
  <tr>
    <td><%= way.way_name %></td>
    <td><%= link_to t('general.show'), way %></td>
    <td><%= link_to t('general.edit'), edit_way_path(way) %></td>
    <td><%= link_to 'Destroy', way, method: :delete, data: { confirm: t('general.confirm') } %></td>
  </tr>
<% end %>
</table>

<br>

<%= link_to 'New Way', new_way_path %>
