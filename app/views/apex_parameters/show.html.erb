<% if notice != nil %>
  <div class="success_alert">
      <span class="closebtn" onclick="this.parentElement.style.display='none';">&times;</span>
      <%= notice %>
  </div>
<% end %>

<p>
  <b>Parameter:</b>
  <%= @apex_parameter.parameter_description_id %>
</p>

<p>
  <b>Name:</b>
  <%= @apex_parameter.parameter.name %>
</p>

<p>
  <b>Project:</b>
  <%= @apex_parameter.project_id %>
</p>

<p>
  <b>line:</b>
  <%= @apex_parameter.parameter.line %>
</p>

<p>
  <b>Low Range:</b>
  <%= @apex_parameter.parameter.range_low %>
</p>

<p>
  <b>High Range:</b>
  <%= @apex_parameter.parameter.range_high %>
</p>

<p>
  <b>Value:</b>
  <%= @apex_parameter.value %>
</p>

<div class="actions">
  <%= link_to t('general.edit'), edit_apex_parameter_path(@apex_parameter), :class => "btn btn-primary" %>
  <%= link_to t('general.back'), apex_parameters_path, :class => "btn btn-primary" %>
</div>
