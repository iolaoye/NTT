<h1>Listing apex_parameters</h1>

<table>
  <tr>
    <th>Parameter</th>
    <th>Value</th>
    <th>Project</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @apex_parameters.each do |apex_parameter| %>
  <tr>
    <td><%= apex_parameter.parameter_id %></td>
    <td><%= apex_parameter.value %></td>
    <td><%= apex_parameter.project_id %></td>
    <td><%= link_to 'Show', apex_parameter %></td>
    <td><%= link_to 'Edit', edit_apex_parameter_path(apex_parameter) %></td>
    <td><%= link_to 'Destroy', apex_parameter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

