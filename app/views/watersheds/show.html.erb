<% if notice != nil %>
  <div class="success_alert">
      <span class="closebtn" onclick="this.parentElement.style.display='none';">&times;</span>
      <%= notice %>
  </div>
<% end %>

<p>
  <b><%= t('general.name') %></b>
  <%= @watershed.name %>
</p>
<div class="actions"> 
<%= link_to t('general.edit'), edit_watershed_path(@watershed) %>
<%= link_to t('general.back'), list_watershed_path(session[:location_id]) %>
</div>