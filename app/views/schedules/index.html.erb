<h1>Listing schedules</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Event order</th>
    <th>Month</th>
    <th>Day</th>
    <th>Year</th>
    <th>Activity</th>
    <th>Apex operation</th>
    <th>Apex crop</th>
    <th>Apex fertilizer</th>
    <th>Apex opv1</th>
    <th>Apex opv2</th>
    <th>Crop schedule</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @schedules.each do |schedule| %>
  <tr>
    <td><%= schedule.id %></td>
    <td><%= schedule.event_order %></td>
    <td><%= schedule.month %></td>
    <td><%= schedule.day %></td>
    <td><%= schedule.year %></td>
    <td><%= schedule.activity_id %></td>
    <td><%= schedule.apex_operation %></td>
    <td><%= schedule.apex_crop %></td>
    <td><%= schedule.apex_fertilizer %></td>
    <td><%= schedule.apex_opv1 %></td>
    <td><%= schedule.apex_opv2 %></td>
    <td><%= schedule.crop_schedule_id %></td>
    <td><%= link_to 'Show', schedule %></td>
    <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
    <td><%= link_to 'Destroy', schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Schedule', new_schedule_path %>
