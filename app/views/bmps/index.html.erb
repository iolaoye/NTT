<% lines_dev = 0 %>
<link href="https://gitcdn.github.io/bootstrap-toggle/2.2.2/css/bootstrap-toggle.min.css" rel="stylesheet">
  <script src="https://gitcdn.github.io/bootstrap-toggle/2.2.2/js/bootstrap-toggle.min.js"></script>
  <head><%#= javascript_include_tag "gen_validatorv4" %></head>
<%= form_for :bmp, :html => { :id => "bmpsForm" }, :url => save_bmps_project_field_scenario_bmps_path(@project, @field, @scenario), method: :post do %>
  <div class="page-header"><h1><%= t('menu.bmps') %></h1></div>
  <div id="flash_messages">
    <% flash.each do |key, value|
      if value.class == String # regular flash notices, alerts, etc. will be strings
        value = [value]
      end
      value.each do |value| %>
        <%= content_tag(:p, value, :class => "flash #{key}") unless value.empty? %>
      <% end %>
    <% end %>
  </div>
  <table class="table table-striped">
    <thead>
      <tr class='gvHeaderStyle'>
        <th><%= t('general.select')%></th>
        <th><%= t('general.name')%></th>
      </tr>
    </thead>
    <% manure_operations = @scenario.operations.where("activity_id = ? and type_id > ?",2,1).count %>
    <% if @project.location.county_id == 0 %>
      <% county_id = 0 %>
    <% else %>
      <% county_id = County.find(@project.location.county_id).id %>
    <% end %>
    <% @bmps.each do |bmp| %>
    <%# debugger %>
      <% if bmp == nil then %>
        <% next %>
      <% end %>
      <tbody>
        <% if !(@project.location.state_id == 25 || @project.location.state_id == 26) && bmp.bmpsublist_id == 4 %>
          <% next %>
        <% end %>
        <% if (manure_operations == 0 && bmp.bmpsublist_id == 18) || (bmp.bmpsublist_id == 18 && county_id != 2939) %>
          <% next %>
        <% end %>

        <% if bmp.bmpsublist_id != 2 && bmp.bmpsublist_id != 11 && bmp.bmpsublist_id != 5 && bmp.bmpsublist_id != 6 && bmp.bmpsublist_id != 7 && bmp.bmpsublist_id != 12 %>
          <% if (bmp.bmpsublist_id == 10 && @scenario.operations.where("(activity_id = ? or activity_id = ?) and org_c = 1 and nh3 > 0", 7, 9).count == 0) %>
          <%# if bmp.bmpsublist_id == 10 %> 
              <% next %>
          <% end %>
          <tr>

            <% if bmp.bmpsublist_id <= 18 || bmp.bmpsublist_id > 20 %>
              <% if bmp.id != nil %>
                <td><%= check_box_tag "select[#{bmp.bmpsublist_id}]", true, value="1"%></td>
              <% else %>
                <td><%= check_box_tag "select[#{bmp.bmpsublist_id}]" %></td>
              <% end %>
              <% if I18n.locale.equal?(:en) %>
                <td style="cursor: pointer" id=<%= "bmp#{bmp.bmpsublist_id}" %> class="bmp"><h4><%= Bmpsublist.find(bmp.bmpsublist_id).name %></h4></td>
              <% else %>
                <td style="cursor: pointer" id=<%= "bmp#{bmp.bmpsublist_id}" %> class="bmp"><h4><%= Bmpsublist.find(bmp.bmpsublist_id).spanish_name %></h4></td>
              <% end %>
            <% end %>
          </tr>
        <%end %>
      </tbody>
      <tbody class="toggable borders" id=<%="#{bmp.bmpsublist_id}"%>>
        <% case bmp.bmpsublist_id %>
          <% when 1 %>
            <tr>
              <td class="indent" colspan="2">
                <% case bmp.depth%>
                  <% when 1 %>
                    <%= radio_button_tag :bmp_cb1, "1", true  %> <%= t('bmp.autoirrigation') %>
                    <%= radio_button_tag :bmp_cb1, "2"%> <%= t('bmp.autofertigation') %>
                  <% when 2 %>
                    <%= radio_button_tag :bmp_cb1, "1" %> <%= t('bmp.autoirrigation') %>
                    <%= radio_button_tag :bmp_cb1, "2", true %> <%= t('bmp.autofertigation') %>
                  <% else %>
                    <%= radio_button_tag :bmp_cb1, "1"%> <%= t('bmp.autoirrigation') %>
                    <%= radio_button_tag :bmp_cb1, "2" %> <%= t('bmp.autofertigation') %>
                <%end%>
              </td>
            </tr>
            <tr>
              <td class="indent2" colspan="2">
                <%= label :irrigation_id, t('bmp.irrigation')  %>
                <% if I18n.locale.equal?(:en) %>
                  <%= collection_select :bmp_ai, :irrigation_id, @irrigations, :id, :name, {:prompt => t('general.select') + " " + t('general.one'), :selected => bmp.irrigation_id}%>
                <% else %>
                  <%= collection_select :bmp_ai, :irrigation_id, @irrigations, :id, :spanish_name, {:prompt => t('general.select') + " " + t('general.one'), :selected => bmp.irrigation_id} %>
                <% end %>
              </td>
            </tr>
            <tr>
              <td class="field indent2" id = "irrigation_efficiency" colspan="2" >
                <%= label :irrigation_efficiency, t('bmp.irrigation_efficiency') + " " + t('bmp.percentage_range') %>
                <%= number_field :bmp_ai, :irrigation_efficiency, :value => (1 - bmp.irrigation_efficiency) * 100, :step => 'any', :min => 0, :max => 100, :class => "gvTitleStyleNumber" %>
                <% if bmp.irrigation_id == 1 %> <% range = "70-80%" %>
                  <% elsif bmp.irrigation_id == 2 %> <% range = "50-70%" %>
                  <% elsif bmp.irrigation_id == 3 %> <% range = "80-95%" %>
                  <% elsif bmp.irrigation_id == 7 %> <% range = "80-95%" %>
                  <% elsif bmp.irrigation_id == 8 %> <% range = "50-70%" %>
                <% end %>
                <input type="text" readonly="" style="border:none;" id="bmp_ai_irrigation_efficiency_lable" value=""<%=range%>>
              </td>
            </tr>
            <tr>
              <td class="field indent2" id = "frequency" colspan="2" >
                <%= label :days, t('bmp.irrigation_frequency') %>
                <%= number_field :bmp_ai, :days, :value => bmp.days, :min => 0, :max => 365, :class => "gvTitleStyleNumber" %>
                <%= t('bmp.days_range') %>
                </td>
            </tr>
            <tr>
              <td class="indent2" colspan="2">
                <%= label :water_stress_factor, t('bmp.water_stress_factor') + " " + t('bmp.zero_to_hundred') %>
                <%= number_field :bmp_ai, :water_stress_factor, :value => bmp.water_stress_factor * 100, :step => 'any', :min => 0, :max => 100, :class => "gvTitleStyleNumber" %>
                <strong><a href="#" id="trigger" style="color:blue;">(?)</a></strong>
              </td>
            </tr>
            <tr>
              <td class="field indent2" id = "maximum_single_application" colspan="2" >
               <%= label :days, t('bmp.maximum_single_application') + " (" + " 1-15 in.)"%>
               <%= number_field :bmp_ai, :maximum_single_application, :value => bmp.maximum_single_application, :step => 'any', :min => 1, :max => 15, :class => "gvTitleStyleNumber" %>
              </td>
            </tr>
            <tr>
              <!--<td class="field indent2" id = "reservoir_area" style="display: none;" colspan="2">
               <%= label :days, t('bmp.reservoir_area') + " (" + t('bmp.acres') + ")"%>
               <%= number_field :bmp_ai, :area, :value => bmp.area, :class => "gvTitleStyleNumber" %>
              </td>-->
            </tr>

            <tr>
              <% if bmp.depth == 1 %>
                <td class="field indent2" id = "td_n_rate" colspan="2" style="display: none">
              <% else %>
                <td class="field indent2" id = "td_n_rate" colspan="2">
              <% end %>
               <%= label :days, t('result.annual') + " " + t('operation.application_rate') + " " + t('activerecord.attributes.apex.for') + " " + t('bmp.autofertigation') + " (" + t('bmp.lbs') + "/" + t("bmp.acres") + ")"%>
               <%= number_field :bmp_ai, :n_rate, :value => bmp.dry_manure, :step => 'any', :min => 0.00, :class => "gvTitleStyleNumber" %>
               <%#= label_tag :application_rate, t('bmp.application_rate_html'), :style => "color:blue" %>
              </td>
            </tr>

          <% if lines_dev==1 then %>
          <tr>
            <td class="indent2" colspan="2">
              <%= label :crop_id, t('operation.crop') %>
              <% if I18n.locale.equal?(:en) %>
                  <%= collection_select :operation, :crop_id, @crops.all, :id, :name,  {:prompt => t('general.select')} %>
              <% else %>
                  <%= collection_select :bmp_ccr, :crop_id, @crops.all, :id, :spanish_name, {:prompt => t('general.select')} %>
              <% end %>
            </td>
          </tr>
          <% end %>

          <tr>
          <td class="indent2" colspan="2">
          <table>
          <tr colspan="5">
           <td></td>
           <td style="text-align: center; vertical-align: middle;">Start Month</td>
           <td style="text-align: center; vertical-align: middle;">Start Day</td>
           <td style="text-align: center; vertical-align: middle;">End Month</td>
           <td style="text-align: center; vertical-align: middle;">End Day</td>
           </tr>
           <tr colspan="5">
            <% debugger %>
            <% @crop_arr.each do |cp| %>
            <% debugger %> 
                <td><%= check_box_tag  "mycrop[#{cp[:id]}]", true, value="Hello"%> <%= cp[:name] %></td>
                <td><%= number_field :bmp_ai,"sm[#{cp[:id]}", :value => cp[:start_month],  :step => 'any', :min => 1, :max => 12, :class => "gvSmallStyleNumber" %></td>
                <td>
                <%= number_field :bmp_ai,"sd[#{cp[:id]}", :value => cp[:start_day], :min => 1, :max => 365, :class => "gvSmallStyleNumber" %>
                </td>
                <td><%= number_field :bmp_ai,"dm[#{cp[:id]}", :value => cp[:end_month], :step => 'any', :min => 1, :max => 12, :class => "gvSmallStyleNumber" %></td>
                <td>
                <%= number_field :bmp_ai,"ed[#{cp[:id]}", :value => cp[:end_day],  :min => 1, :max => 365, :class => "gvSmallStyleNumber" %>
                </td>
                <td>
               <%= hidden_field_tag "my_crop_id", cp[:id]%>
                </td>
 
              </tr>
            <% end  %>
           </tr>
          </table>
          </td>
           </tr>

          <% when 3 %>
            <tr>
              <td class="field indent" id = "depth" colspan="2" >
                <%= label :depth, t('bmp.depth') + " (" + t('bmp.ft')  + ")"%>
                <%= number_field :bmp_td, :depth, :value => bmp.depth, :step => 'any', :class => "gvTitleStyleNumber", :min => 0 %>
              </td>
            </tr>
            <tr>
              <td class="field indent" colspan="3"><%= t("bmp.drainage_management_quiestion") %></td>
            </tr>
            <tr>
              <td class="field indent" colspan="3">
                <% if bmp.irrigation_id == 1 %>
                  <%= check_box_tag :irrigation_id, bmp.irrigation_id, true %><%="  " + t('bmp.tile_bioreactors')%>
                <% else %>
                  <%= check_box_tag :irrigation_id, bmp.irrigation_id, false %><%="  " + t('bmp.tile_bioreactors')%>
                 <% end %>

                <% if bmp.crop_id == 1 %>
                  <%= check_box_tag :crop_id, bmp.irrigation_id, true %><%="  " + t('bmp.drainage_management')%>
                <% else %>
                  <%= check_box_tag :crop_id, bmp.irrigation_id, false %><%="  " + t('bmp.drainage_management')%>
                 <% end %>
              </td>
            </tr>
          <% when 4 %>
            <% if @project.location.state_id == 25 || @project.location.state_id == 26 %>
              <tr>
                <td class="indent" colspan="2">
                  <% case bmp.depth%>
                    <% when 4 %>
                      <%= radio_button_tag :bmp_cb2, "4", true %> <%= t('pdf.ppdi')%> </br>
                      <%= radio_button_tag :bmp_cb2, "5"%> <%= t('pdf.ppds') %></br>
                      <%= radio_button_tag :bmp_cb2, "6"%> <%= t('pdf.ppde') %></br>
                      <%= radio_button_tag :bmp_cb2, "7"%> <%= t('pdf.ppti') %></br>
                    <% when 5 %>
                      <%= radio_button_tag :bmp_cb2, "4" %> <%= t('pdf.ppdi') %></br>
                      <%= radio_button_tag :bmp_cb2, "5", true %> <%= t('pdf.ppds')%></br>
                      <%= radio_button_tag :bmp_cb2, "6"%> <%= t('pdf.ppde') %></br>
                      <%= radio_button_tag :bmp_cb2, "7"%> <%= t('pdf.ppds') %></br>
                    <% when 6 %>
                      <%= radio_button_tag :bmp_cb2, "4" %> <%= t('pdf.ppdi') %></br>
                      <%= radio_button_tag :bmp_cb2, "5"%> <%= t('pdf.ppds') %></br>
                      <%= radio_button_tag :bmp_cb2, "6", true %> <%= t('pdf.ppde') %></br>
                      <%= radio_button_tag :bmp_cb2, "7"%> <%= t('pdf.ppti') %></br>
                    <% when 7 %>
                      <%= radio_button_tag :bmp_cb2, "4" %> <%= t('pdf.ppdi') %></br>
                      <%= radio_button_tag :bmp_cb2, "5"%> <%= t('pdf.ppds') %></br>
                      <%= radio_button_tag :bmp_cb2, "6"%> <%= t('pdf.ppde') %></br>
                      <%= radio_button_tag :bmp_cb2, "7", true %> <%= t('pdf.ppti') %></br>
                    <% else %>
                      <%= radio_button_tag :bmp_cb2, "4"%> <%= t('pdf.ppdi') %></br>
                      <%= radio_button_tag :bmp_cb2, "5" %> <%= t('pdf.ppds') %></br>
                      <%= radio_button_tag :bmp_cb2, "6"%> <%= t('pdf.ppde') %></br>
                      <%= radio_button_tag :bmp_cb2, "7"%> <%= t('pdf.ppti') %></br>
                    <%end%>
                </td>
            </tr>
            <tr>
              <td class="indent" colspan="2">
                <%= t('bmp.pad') + " " + t('bmp.width') + " (" + t('bmp.ft')  + ")"%>
                <%= number_field :bmp_ppnd, :width, :value => bmp.width, :class => "gvTitleStyleNumber" %>
              </td>
            </tr>
            <tr>
              <td class="indent" colspan="2">
                  <%= t('bmp.sides')  %>
                  <%= number_field :bmp_ppnd, :sides, :value => bmp.sides, :class => "gvTitleStyleNumber" %>
              </td>
            </tr>
            <% if bmp.depth == 6 || bmp.depth == 7 %>
              <tr id = "tr_bmp_pp_area" >
                  <td class="indent" colspan="2">
                    <%= label :area, t('bmp.reservoir_area') + " (" + t('bmp.acres')  + ")"%>
                    <%= number_field :bmp_ppnd, :area, :value => bmp.area, :class => "gvTitleStyleNumber" %>
                    </td>
              </tr>
            <% else %>
              <tr id = "tr_bmp_pp_area" style="display:none" >
                    <td class="indent" colspan="2">
                      <%= label :area, t('bmp.reservoir_area') + " (" + t('bmp.acres')  + ")"%>
                      <%= text_field :bmp_ppnd, :area, :value => bmp.area %>
                    </td>
                </tr>
              <% end %>
            <% end %>
          <% when 8 %>
            <tr>
              <td class="field indent" colspan="2" id = "area" colspan="2">
                <%= label :depth, t('pdf.area') + " (" + t('bmp.acres')  + ")"%>
                <%= number_field :bmp_wl, :area, :value => bmp.area, :step => 'any', :min => 0, :class => "gvTitleStyleNumber"%>
              </td>
            </tr>
            <tr >
              <td class="indent" colspan="2">
                <% if bmp.sides == 1 %>
                  <%= t('bmp.additional_land')%>  <input type="checkbox" id="bmp_wl_buffer_land", name="bmp_wl[buffer_land]", checked="" data-toggle="toggle" data-size="mini" data-onstyle="success" data-off="No" data-on="Yes" >
                <% else %>
                  <%= t('bmp.additional_land').sub('buffer', 'wetland')%>  <input type="checkbox" id="bmp_wl_buffer_land", name="bmp_wl[buffer_land]", data-toggle="toggle" data-size="mini" data-onstyle="success" data-off="No" data-on="Yes" >
                <% end %>
              </td>
            </tr>
          <% when 9 %> <%# ponds %>
            <tr>
              <td class="field indent" colspan="2" id = "fraction_control_by_pond" >
                <%= label_tag :control, t('bmp.area_control_html') + " " + t('bmp.zero_to_one')  %>
                <% if bmp.irrigation_efficiency  != nil %>
                  <% ie = bmp.irrigation_efficiency %>
                <% else %>
                  <% ie = 0 %>
                <% end %>
                <%= number_field :bmp_pnd, :irrigation_efficiency, :value => ie, :step => '0.01', :min => 0, :max => 1, :class => "gvTitleStyleNumber" %>
              </td>
            </tr>
          <% when 10 %> <%# Stream Fencing %>
            <tr>
              <td class="indent" colspan="2" style='display:none'>
                <%= label :depth, t('operation.animals')  %>
                <%= number_field :bmp_sf, :number_of_animals, :value => bmp.number_of_animals, :min => 0, :class => "gvTitleStyleNumber" %>
              </td>
            </tr>
            <tr>
              <td class="indent" colspan="2" >
                <div><%#= label :fence, t('bmp.fence') %></div>
                  <%= label :width, t('bmp.distance') + "(" + t('bmp.ft') + ")" %>
                  <%= number_field :bmp_sf, :width, :value => bmp.width, :style=>"width:70px", :min => 0, :class => "gvTitleStyleNumber" %>
                  <%= label :width_zero, t('bmp.zero_fence'), class: "title_label"%>
                  <!-- <div style='display:none'>
                    <%= label :length, t('bmp.length') + "(" + t('bmp.ft') + ")" %>
                    <%= number_field :bmp_sf, :depth, :value => bmp.depth, :style=>"width:70px", :step => '0.1', :min => 0, :class => "gvTitleStyleNumber" %>
                  </div> -->
                </td>
            </tr>
            <tr>
              <td class="indent" colspan="2" style='display:none'>
                <%= label :animal_id, t('bmp.animals') %>
                <% if I18n.locale.equal?(:en) %>
                  <%= collection_select :bmp_sf, :animal_id, Fertilizer.where(:animal => true, :status => true), :id, :name, {:prompt => t('general.select') + " " + t('general.one'), :required => true, :selected => bmp.animal_id} %>
                <% else %>
                  <%= collection_select :bmp_sf, :animal_id, Fertilizer.where(:animal => true, :status => true), :id, :spanish_name, {:prompt => t('general.select') + " " + t('general.one'), :required => true, :selected => bmp.animal_id} %>
                <% end %>
              </td>
            </tr>
            <tr>
              <td class="indent2" colspan="2" style='display:none'>
              <%= label :dry_manure, t('bmp.dry_manure') + " (" + t('bmp.lbs')  + ")"%>
              <%= number_field :bmp_sf, :dry_manure, :value => bmp.dry_manure, :step => 'any', :min => 0, :class => "gvTitleStyleNumber" %>
              </td>
            </tr>
            <tr>
              <td class="indent2" colspan="2" style='display:none'>
              <%= label :no3_n, t('bmp.no3_n_html') + " " + t('bmp.zero_to_one') %>
              <%= number_field :bmp_sf, :no3_n, :value => bmp.no3_n, :step => '0.001', :min => 0.00, :max => 1.00, :class => "gvTitleStyleNumber" %>
              </td>
            </tr>
            <tr>
              <td class="indent2" colspan="2" style='display:none'>
              <%= label :po4_p, t('bmp.po4_p_html') + " " + t('bmp.zero_to_one') %>
              <%= number_field :bmp_sf, :po4_p, :value => bmp.po4_p, :step => '0.001', :min => 0.00, :max => 1.00, :class => "gvTitleStyleNumber" %>
              </td>
            </tr>
            <tr>
              <td class="indent2" colspan="2" style='display:none'>
              <%= label :org_n, t('bmp.org_n_html') + " " + t('bmp.zero_to_one') %>
              <%= number_field :bmp_sf, :org_n, :value => bmp.org_n, :step => '0.001', :min => 0.00, :max => 1.00, :class => "gvTitleStyleNumber" %>
              </td>
            </tr>
            <tr>
              <td class="indent2" colspan="2" style='display:none'>
              <%= label :org_p, t('bmp.org_p_html') + " " + t('bmp.zero_to_one') %>
              <%= number_field :bmp_sf, :org_p, :value => bmp.org_p, :step => '0.001', :min => 0.00, :max => 1.00, :class => "gvTitleStyleNumber" %>
              </td>
            </tr>
          <% when 13 %>
            <tr>
              <td class="indent" colspan="2">
                <% case bmp.depth%>
                  <% when 13 %>
                    <%= radio_button_tag :bmp_cb3, "13", true  %> <%= t('pdf.filter_strip') %>
                    <%= radio_button_tag :bmp_cb3, "12"%> <%= t('pdf.riparian_forest') %>
                  <% when 12 %>
                    <%= radio_button_tag :bmp_cb3, "13" %> <%= t('pdf.filter_strip') %>
                    <%= radio_button_tag :bmp_cb3, "12", true %> <%= t('pdf.riparian_forest') %>
                  <% else %>
                    <%= radio_button_tag :bmp_cb3, "13"%> <%= t('pdf.filter_strip') %>
                    <%= radio_button_tag :bmp_cb3, "12" %> <%= t('pdf.riparian_forest') %>
                <%end%>
              </td>
            </tr>
            <tr id = "tr_fs_vegetation" <%= "style=display:none" if bmp.depth == 12%>>
              <td class="indent2" colspan="2">
                <%= label :crop_id, t('operation.crop') %>
                <% if I18n.locale.equal?(:en) %>
                    <%= collection_select :bmp_fs, :crop_id, Crop.where(:type1 => "FS"), :id, :name, {:prompt => t('bmp.select_vege'), :selected => bmp.crop_id}%>
                <% else %>
                    <%= collection_select :bmp_fs, :crop_id, Crop.where(:type1 => "FS"), :id, :spanish_name, {:prompt => t('bmp.select_vege'), :selected => bmp.crop_id} %>
                <% end %>
              </td>
            </tr>
            <tr>
              <td class="indent2" colspan="2">
                <%= t('pdf.area') + " (" + t('bmp.acres') + ")"%>
                <%= number_field :bmp_fs, :area, :value => bmp.area, :step => 'any', :class => "gvTitleStyleNumber", :min => "0" %>
                <%= label :area, " (" + t('general.optional') + ")", :style => "color:blue" %>
                </td>
            </tr>
            <tr>
              <td class="indent2" colspan="2">
                <%= t('pdf.filter_strip').sub('Buffer', 'Strip') + " " + t('bmp.width') + " (" + t('bmp.ft') + ")" %>
                <%= number_field :bmp_fs, :width, :value => bmp.width, :step => 'any', :min => 0, :class => "gvTitleStyleNumber" %>
              </td>
            </tr>
            <tr id = "tr_grass_field_portion" <%= "style=display:none" if bmp.depth == 13%>>
              <td class="indent2" colspan="2">
                <%= t('pdf.riparian_forest').sub('Buffer', 'Strip') + " " + t('bmp.width') + " (" + t('bmp.ft') + ")" %>
                <%= number_field :bmp_fs, :grass_field_portion, :value => bmp.grass_field_portion, :step => 'any', :min => 0, :class => "gvTitleStyleNumber" %>
              </td>
            </tr>
            <tr style='display:none'>
              <td class="indent2" colspan="2">
                <%= t('bmp.slope_ratio_label') + " " + t('bmp.quarter_to_one') %>
