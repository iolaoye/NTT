            <td class="gvTotalText" style="background-color:#DEFFFD"><%#= sprintf("%8.2f", @aplcat_results2.AGR)%></td>
            <%# difference1 = @aplcat_results1.AGR - @aplcat_results2.AGR %>
            <%# reduction1 = difference1 / @aplcat_results1.AGR * 100 %>
            <td class= "gvTotalText" style="border-right: thick double #ff0000; background-color:#DEFFFD"><%#= sprintf("%8.1f", difference1) + " (" + sprintf("%8.1f", reduction1) + ")" %></td>
