<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="String"><%= t('result.description') %></Data></Cell>
        <% if session[:simulation] == 'scenario' %>
				  <% if !@present1 %>
            <Cell><Data ss:Type="String"><%= t('result.scenario1') %></Data></Cell>
          <% else %>
            <Cell><Data ss:Type="String"><%=Scenario.find(@results1[0].scenario_id).name%></Data></Cell>
          <% end %>
				  <Cell><Data ss:Type="String"><%= "±" %></Cell></Data>
				  <% if !@present2 %>
            <Cell><Data ss:Type="String"><%= t('result.scenario2') %></Cell></Data>
          <% else %>
            <Cell><Data ss:Type="String"><%=Scenario.find(@results2[0].scenario_id).name%></Cell></Data>
          <% end %>
				<% else %>
          <% if !@present1 %>
            <Cell><Data ss:Type="String"><%= t('result.scenario1') %></Cell></Data>
          <% else %>
            <Cell><Data ss:Type="String"><%=Watershed.find(@results1[0].watershed_id).name%></Cell></Data>
          <% end %>
            <Cell><Data ss:Type="String"><%= "±" %></Cell></Data>
          <% if !@present2 %>
            <Cell><Data ss:Type="String"><%= t('result.scenario2') %></Cell></Data>
          <% else %>
            <Cell><Data ss:Type="String"><%=Watershed.find(@results2[0].watershed_id).name%></Cell></Data>
          <% end %>
        <% end %>
        <Cell><Data ss:Type="String"><%= "±" %></Cell></Data>
        <Cell><Data ss:Type="String"><%= t('result.difference') %></Cell></Data>
        <Cell><Data ss:Type="String"><%= t('result.reduction') %></Cell></Data>
        <Cell><Data ss:Type="String"><%= t('result.total_area') %></Cell></Data>
        <% if session[:simulation] == 'scenario' %>
          <% if !@present3 %>
            <Cell><Data ss:Type="String"><%= t('result.scenario3') %></Cell></Data>
          <% else %>
            <Cell><Data ss:Type="String"><%=Scenario.find(@results3[0].scenario_id).name%></Cell></Data>
          <% end %>
        <% else %>
          <% if !@present3 %>
            <Cell><Data ss:Type="String"><%= t('result.scenario3') %></Cell></Data>
          <% else %>
            <Cell><Data ss:Type="String"><%=Watershed.find(@results3[0].watershed_id).name%></Cell></Data>
          <% end %>
        <% end %>
        <Cell><Data ss:Type="String"><%= "±" %></Cell></Data>
        <Cell><Data ss:Type="String"><%= t('result.difference') %></Cell></Data>
        <Cell><Data ss:Type="String"><%= t('result.reduction') %></Cell></Data>
        <Cell><Data ss:Type="String"><%= t('result.total_area') %></Cell></Data>
      </Row>
    </Table>
  </Worksheet>
</Workbook>
