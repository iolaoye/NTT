<h1>Listing bmplists</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @bmplists.each do |bmplist| %>
  <tr>
    <td><%= bmplist.name %></td>
    <td><%= link_to 'Show', bmplist %></td>
    <td><%= link_to t('general.edit'), edit_bmplist_path(bmplist) %></td>
    <td><%= link_to 'Destroy', bmplist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

