<h1>Listing stations</h1>

<table>
  <tr>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @stations.each do |station| %>
  <tr>
    <td><%= link_to t('general.show'), station %></td>
    <td><%= link_to t('general.edit'), edit_station_path(station) %></td>
    <td><%= link_to 'Destroy', station, method: :delete, data: { confirm: t('general.confirm') } %></td>
  </tr>
<% end %>
</table>

