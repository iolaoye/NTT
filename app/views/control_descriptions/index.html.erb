<h1>Listing control_descriptions</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Line</th>
    <th>Column</th>
    <th>Code</th>
    <th>Name</th>
    <th>Range low</th>
    <th>Range high</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @control_descriptions.each do |control_description| %>
  <tr>
    <td><%= control_description.id %></td>
    <td><%= control_description.line %></td>
    <td><%= control_description.column %></td>
    <td><%= control_description.code %></td>
    <td><%= control_description.name %></td>
    <td><%= control_description.range_low %></td>
    <td><%= control_description.range_high %></td>
    <td><%= link_to 'Show', control_description %></td>
    <td><%= link_to 'Edit', edit_control_description_path(control_description) %></td>
    <td><%= link_to 'Destroy', control_description, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

