<%= form_for [@project, @field, @scenario, @animal_transport] do |f| %>
  <% if @animal_transport.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@animal_transport.errors.count, "error") %> prohibited this animal_transport from being saved:</h2>
      <ul>
      <% @animal_transport.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
      </ul>
    </div>
  <% end %>
  <br>
  <table class="table table-striped">
    <tbody class="toggable borders">
      <tr>
        <td class="indent" colspan="2">
          <div class="field">
            <%=  t('aplcat.freqtrip') %>
            <%= number_field :freqtrip, class: 'gvTitleStyleNumber', :title => t('aplcat_tooltip.freqtrip'), :step => 'any', :min => 1, :max => 10 %>
          </div>
        </td>
      </tr>
      <tr>
        <td class="indent" colspan="2">
          <div class="field">
            <%= t('aplcat.cattlepro') %><br>
            <%= radio_button_tag :cattlepro, "1", :checked => "checked"%> <%= t('graze.yes') %><br>
            <%= radio_button_tag :cattlepro, "2"%> <%= t('graze.no') %><br>
          </div>
        </td>
      </tr>
      <tr>
        <td class="indent" colspan="2">
          <div class="field">
            <%=  t('aplcat.purpose') %><br>
            <%= radio_button_tag :purpose, "1", :checked => "checked"%> <%= t('aplcat.buying') %><br>
            <%= radio_button_tag :purpose, "2"%> <%= t('aplcat.selling') %><br>
            <%#= text_field :purpose, :maxlength => "25", :title => t('aplcat_tooltip.purpose'), :placeholder => "Enter Text Here" %>
          </div>
        </td>
      </tr>
      <tr>
        <td class="indent" colspan="2">
          <div class="field">
            <%= t("Transportation:") %><br>
            <%= radio_button_tag :trans, "1", :checked => "checked"%> <%= t('aplcat.trans_feeder') %><br>
            <%= radio_button_tag :trans, "2"%> <%= t('aplcat.trans_finish') %><br>
          </div>
        </td>
      </tr>
      <tr>
        <td class="indent" colspan="2">
          <div class="field">
            <%=  t('aplcat.categories_trans') %>
            <%= f.number_field :categories_trans, :min => 1, :max => 10, :step => 1 %>
          </div>
          


          <div>
            <table class="toggable borders">
              <tr><td colspan="5" style="display:none"><%= 'Average market weight of each animal (lb)' %></td></tr>
              <tr>
                <td class="indent" id="tdweight1" <%="style=display:none;" unless @categories[0] != nil %> <%=number_field_tag "weight1", @categories[0].weight unless @categories != nil %>
                </td>
                <td class="indent" id="tdweight2" <%=style="display:none" unless @categories[1] != nil %>><%= number_field_tag "weight2", @categories[1].weight unless @categories != nil %></td>
                <td class="indent" id="tdweight3" <%=style="display:none" unless @categories[1] != nil %>><%= number_field_tag "weight3", @categories[2].weight unless @categories != nil %></td>
                <td class="indent" colspan="2" style="display:none"><%= number_field_tag "weight4", class: 'gvCellStyleNumber', :min => 1, :step => 1 %></td>
                <td class="indent" colspan="2" style="display:none"><%= number_field_tag "weight5", class: 'gvCellStyleNumber', :min => 1, :step => 1 %></td>
                <td class="indent" colspan="2" style="display:none"><%= number_field_tag "weight6", class: 'gvCellStyleNumber', :min => 1, :step => 1 %></td>
                <td class="indent" colspan="2" style="display:none"><%= number_field_tag "weight7", class: 'gvCellStyleNumber', :min => 1, :step => 1 %></td>
                <td class="indent" colspan="2" style="display:none"><%= number_field_tag "weight8", class: 'gvCellStyleNumber', :min => 1, :step => 1 %></td>
                <td class="indent" colspan="2" style="display:none"><%= number_field_tag "weight9", class: 'gvCellStyleNumber', :min => 1, :step => 1 %></td>
                <td class="indent" colspan="2" style="display:none"><%= number_field_tag "weight10", class: 'gvCellStyleNumber', :min => 1, :step => 1 %></td>
              </tr>
              <tr><td colspan="5" style="display:none"><%= 'Number of animals in each category' %></td></tr>
              <tr>
                <td class="indent" <%=style="display:none" unless @categories[0] != nil %>><%= number_field_tag "animals1", @categories[0].weight unless @categories != nil %></td>              
                <td class="indent" <%=style="display:none" unless @categories[0] != nil %>><%= number_field_tag "animals2", @categories[0].weight unless @categories != nil %></td>
                <td class="indent" <%=style="display:none" unless @categories[0] != nil %>><%= number_field_tag "animals3", @categories[0].weight unless @categories != nil %></td>
                <td class="indent" colspan="2" style="display:none"><%= number_field_tag "animals3", class: 'gvCellStyleNumber', :min => 1, :step => 1 %></td>
                <td class="indent" colspan="2" style="display:none"><%= number_field_tag "animals4", class: 'gvCellStyleNumber', :min => 1, :step => 1 %></td>
                <td class="indent" colspan="2" style="display:none"><%= number_field_tag "animals5", class: 'gvCellStyleNumber', :min => 1, :step => 1 %></td>
                <td class="indent" colspan="2" style="display:none"><%= number_field_tag "animals6", class: 'gvCellStyleNumber', :min => 1, :step => 1 %></td>
                <td class="indent" colspan="2" style="display:none"><%= number_field_tag "animals7", class: 'gvCellStyleNumber', :min => 1, :step => 1 %></td>
                <td class="indent" colspan="2" style="display:none"><%= number_field_tag "animals8", class: 'gvCellStyleNumber', :min => 1, :step => 1 %></td>
                <td class="indent" colspan="2" style="display:none"><%= number_field_tag "animals9", class: 'gvCellStyleNumber', :min => 1, :step => 1 %></td>
                <td class="indent" colspan="2" style="display:none"><%= number_field_tag "animals10", class: 'gvCellStyleNumber', :min => 1, :step => 1 %></td>
            </tr>
            </table>
          </div>
        </td>
      </tr>
      <tr>
        <td class="indent" colspan="2">
          <div class="field">
            <%=  t('aplcat.categories_slaug') %>
            <%= number_field :categories_slaug, class: 'gvTitleStyleNumber', :min => 50.0, :max => 2000.0, :step => '0.1' %>
          </div>
        </td>
      </tr>
        </td>
      </tr>
      <tr>
        <td class="indent" colspan="2">
          <div class="field">
            <%=  t('aplcat.avg_marweight') %>
            <%= number_field :avg_marweight, class: 'gvTitleStyleNumber', :min => 1, :title => t('aplcat_tooltip.avg_marweight'), :step => 'any' %>
          </div>
        </td>
      </tr>
      <tr>
        <td class="indent" colspan="2">
          <div class="field">
            <%=  t('aplcat.num_animal') %>
            <%= number_field :num_animal, class: 'gvTitleStyleNumber', :min => 1, :title => t('aplcat_tooltip.num_animal'), :step => 'any' %>
          </div>
        </td>
      </tr>
      <tr>
        <td class="indent" colspan="2">
          <div class="field">
            <%=  t('aplcat.mortality_rate') %>
            <%= number_field :mortality_rate, class: 'gvTitleStyleNumber', :min => 0.01, :max => 5.00, :title => t('aplcat_tooltip.mortality_rate'), :step => '0.01' %>
          </div>
        </td>
      </tr>
      <tr>
        <td class="indent" colspan="2">
          <div class="field">
            <%=  t('aplcat.distance') %>
            <%= number_field :distance, class: 'gvTitleStyleNumber', :min => 0.01, :title => t('aplcat_tooltip.distance'), :step => '0.01' %>
          </div>
        </td>
      </tr>
      <tr>
        <td class="indent" colspan="2">
          <div class="field">
            <%=  t('aplcat.trailers') %>
            <%= collection_select :trailer, :trailer_id, Trailer.all, :id, :description, {:prompt => t('general.select') + " " + t('general.one'), :selected => @animal_transport.trailer_id}%>
          </div>
        </td>
      </tr>
      <tr>
        <td class="indent" colspan="2">
          <div class="field">
            <%=  t('aplcat.trucks') %>
            <%= collection_select :truck, :truck_id, Truck.all, :id, :description, {:prompt => t('general.select') + " " + t('general.one'), :selected => @animal_transport.truck_id}%>
          </div>
        </td>
      </tr>
      <tr>
        <td class="indent" colspan="2">
          <div class="field">
            <%=  t('aplcat.fuel_type') %>
           <%= collection_select :fuel, :fuel_id, Fuel.all, :id, :description, {:prompt => t('general.select') + " " + t('general.one'), :selected => @animal_transport.fuel_id}%>
          </div>
        </td>
      </tr>
      <tr>
        <td class="indent" colspan="2">
          <div class="field">
            <%=  t('aplcat.same_vehicle') %><br>
            <%= radio_button_tag :same_vehicle, "1", :checked => "checked"%> <%= t("Yes") %><br>
            <%= radio_button_tag :same_vehicle, "2"%> <%= t("No") %><br>
          </div>
        </td>
      </tr>
      <tr>
        <td class="indent" colspan="2">
          <div class="field">
            <%=  t('aplcat.loading') %><br>
            <%= radio_button_tag :loading, "1", :checked => "checked"%> <%= t('aplcat.thoughtless') %><br>
            <%= radio_button_tag :loading, "2"%> <%= t('aplcat.arranged') %><br>
            <%= radio_button_tag :loading, "3"%> <%= t('aplcat.floor_space') %><br>
          </div>
        </td>
      </tr>
      <tr>
        <td class="indent" colspan="2">
          <div class="field">
            <%=  t('aplcat.carcass') %>
            <%= number_field :carcass, class: 'gvTitleStyleNumber', :min => 30.0, :max => 65.0, :title => t('aplcat_tooltip.carcass'), :step => '0.1' %>
          </div>
        </td>
      </tr>
      <tr>
        <td class="indent" colspan="2">
          <div class="field">
            <%=  t('aplcat.boneless_beef') %>
            <%= number_field :boneless_beef, class: 'gvTitleStyleNumber', :min => 30.0, :max => 65.0, :title => t('aplcat_tooltip.boneless_beef'), :step => '0.1' %>
          </div>
        </td>
      </tr>
    </tbody>
  </table>
  <div class="actions">
    <%= f.submit:class => "btn btn-primary" %>
    <%= link_to t('general.back'), edit_project_field_scenario_aplcat_parameter_path(@project, @field, @scenario, 1), :class => "btn btn-primary"  %>
  </div>
<% end %>
