<h1>Listing parameters</h1>

<table>
  <tr>
    <th>id</th>
    <th>Param. Number</th>
    <th>Default Value</th>
    <th>State_ID</th>
  </tr>

<% @parameters.each do |parameter| %>
  <tr>
    <td><%=parameter.id%></td>
    <td><%=parameter.number%></td>
    <td><%=parameter.default_value%></td>
    <td><%=parameter.state_id%></td>
    <td><%= link_to t('general.show'), parameter %></td>
    <td><%= link_to t('general.edit'), edit_parameter_path(parameter) %></td>
    <td><%= link_to 'Destroy', parameter, method: :delete, data: { confirm: t('general.confirm') } %></td>
  </tr>
<% end %>
</table>

<br>

<%= link_to 'New Parameter', new_parameter_path %>
